<!DOCTYPE html>
<html lang="en">

    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Product</title>
    </head>

    <body>
        <section>
            <table border="1" cellpadding="10">
                <thead>
                    <tr>
                        <% for(let title of headline){ %>
                            <th>
                                <%=title %>
                            </th>
                            <% } %>
                    </tr>
                </thead>
                <tbody>
                    <% for(let item of productList){ %>
                        <tr>
                            <td>
                                <%=item.name %>
                            </td>
                            <td>
                                <%=item.count %>
                            </td>
                            <td>
                                <%=item.description %>
                            </td>
                        </tr>
                        <% } %>
                </tbody>
            </table>
        </section>
    </body>

</html>