<!DOCTYPE html>
<html lang="en">

    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Info</title>
    </head>

    <body>
        <h1>
            <%= title %>
        </h1>
        <ul>
            <% if(Array.isArray(list)){ %>
                <% for(let item of list){ %>
                    <li><a href="<%= item.url %>">
                            <%= item.name %>
                        </a></li>
                    <% } %>
                        <% } else { %>
                            <p>
                                <%=list %>
                            </p>
                            <% } %>
        </ul>
    </body>

</html>